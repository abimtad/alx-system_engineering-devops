# abeni

#!/usr/bin/env bash
# This script gets Nginx running and to start listening on
# port 80 of all available IP addresses

rm /etc/nginx/sites-enabled/default
ln -s /etc/nginx/sites-available/default /etc/nginx/sites-enabled/default
service nginx restartw

